<plugin>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.eclipse.xtext.ui.Examples" 
				  name="%XtextExamples_category" 
				  parentCategory="org.eclipse.ui.Examples" />
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.eclipse.xtext.project.Examples" 
				  name="%XtextExamples_category2" 
				  parentCategory="org.eclipse.xtext.projectwiz" />
	</extension>

	<!-- ========= org.eclipse.xtext.example.domainmodel ========= -->
	<extension point="org.eclipse.ui.newWizards">
		<wizard	id="org.eclipse.xtext.examples.NewDomainModelExampleWizard" 
			name="%DomainExample_name" 
			class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard" 
			category="org.eclipse.ui.Examples/org.eclipse.xtext.ui.Examples"
			icon="icons/genproject.gif" 
			project="true">
			<description>%DomainExample_description</description>
		</wizard>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<wizard	id="org.eclipse.xtext.examples.NewDomainModelExampleWizard" 
			name="%DomainExample_name" 
			class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard" 
			category="org.eclipse.xtext.projectwiz/org.eclipse.xtext.project.Examples"
			icon="icons/genproject.gif" 
			project="true">
			<description>%DomainExample_description</description>
		</wizard>
	</extension>
	<extension point="org.eclipse.emf.common.ui.examples">
	   <example wizardID="org.eclipse.xtext.examples.NewDomainModelExampleWizard" pageImage="icons/genproject.gif">
		 <projectDescriptor name="org.eclipse.xtext.example.domainmodel" contentURI="contents/org.eclipse.xtext.example.domainmodel.zip" description="Parser, Serializer, Linker, etc."/>
		 <projectDescriptor name="org.eclipse.xtext.example.domainmodel.ui" contentURI="contents/org.eclipse.xtext.example.domainmodel.ui.zip" description="A smart editor for the language."/>
		 <projectDescriptor name="org.eclipse.xtext.example.domainmodel.ui.tests" contentURI="contents/org.eclipse.xtext.example.domainmodel.ui.tests.zip" description="JUnit test cases"/>
	  </example>
	</extension>   
   
